{
  "manifest_version": 2,

  "name": "55444.in",
  "description": "This extension is a search plugin for 55444.in apps and SMSGyan",
  "version": "1.0",

  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "emulator.html"
  },
  "permissions": [
    "http://gyan.mobi/",
    "https://gyan.mobi/"
  ]
}
